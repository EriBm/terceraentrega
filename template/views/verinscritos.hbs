 
{{>header}}
     <table class='table table-striped table-hover'> 
        <thead class='thead-dark'>
        <th>Nombre</th>
        <th>Documento de indentidad</th>
        <th>Correo</th>
        <th>Telefono</th>
        <th></th>
        </thead>
        <tbody>
  {{#each inscritos}}
  <tr>
    <td> {{this.nombre}} </td>
    <td>{{this.documento}}</td>
    <td> {{this.correo}}</td>
    <td> {{this.telefono}} </td>
    <td><button class="btn btn-dark" name="id" value="{{this._id}}">Ver detalle</button></td>
    </tr>
  {{/each}}
  </tbody></table>
  {{>footer}}
  